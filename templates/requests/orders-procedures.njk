{% extends "templates/layout/app.html" %} {% block content %}

<main class="main-view">
  <div class="col-12 row mb-2">
    <div class="col-9">
      <input type="text" class="form-control" placeholder="بحث" />
    </div>

    <div class="col-3 row">
      <div class="col-7">
        <button class="btn btn-primary w-100 px-1">بحث</button>
      </div>
      <div class="col-5">
        <button class="btn btn-outline-primary w-100 px-1">
          <i class="bx bx-filter-alt"></i>
          فلتر
        </button>
      </div>
    </div>
  </div>
  <h4 class="mb-4">طلبات الموظفين</h4>
  <nav class="nav-tabs">
    <a href="#" class="active">الاجازات</a>
    <a href="#">مهمة عمل</a>
    <a href="#">السلف</a>
    <a href="#">العهدة</a>
    <a href="#">احتياجات العمل</a>
    <a href="#">صيانة سيارة</a>
  </nav>
  <div class="section">
    {% for i in range(1, 7) -%}
    <div class="pb-2">
      <div
        class="d-flex btn btn-primary-light flex-row bg-med-transparent w-100 justify-content-between"
      >
        <button
          class="btn btn-icon px-2"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#collapseExample{{ i }}"
          aria-expanded="false"
          aria-controls="collapseExample{{ i }}"
        >
          <i class="bx bx-plus-circle"></i>
          مرحلة {{ i }}
        </button>

        <div class="d-flex flex-row gap-2">
          <button class="btn btn-outline-primary btn-icon px-2" type="button">
            <i class="bx bx-edit"></i>
            تعديل
          </button>
          <button
            class="btn btn-outline-danger btn-icon px-2"
            data-bs-toggle="modal"
            data-bs-target="#staticBackdrop"
          >
            <i class="bx bx-trash"></i>
            حذف
          </button>
        </div>
      </div>
      <div class="collapse" id="collapseExample{{ i }}">
        <div class="section my-2">
          <table class="table table-borderless">
            <thead>
              <th>مدير القسم الوظيفي</th>
              <th>الموظف</th>
              <th>موافقة</th>
              <th>اعتماد</th>
              <th>مدة التجاول</th>
              <th>مدير</th>
            </thead>
            <tbody>
              <th>
                <input
                  type="text"
                  placeholder="اختر مدير القسم"
                  class="limit-3 form-control"
                />
              </th>
              <th>
                <select name="employee" class="form-select limit-3">
                  <option value="1">موظف 1</option>
                  <option value="2">موظف 2</option>
                  <option value="3">موظف 3</option>
                  <option value="4">موظف 4</option>
                </select>
              </th>
              <th>
                <input
                  class="form-check-input"
                  type="checkbox"
                  value=""
                  id="flexCheckDefault"
                />
              </th>
              <th>
                <input
                  class="form-check-input"
                  type="checkbox"
                  value=""
                  id="flexCheckDefault"
                />
              </th>
              <th>
                <input
                  type="number"
                  placeholder="ساعة"
                  class="limit-3 form-control"
                />
              </th>
              <th>
                <select name="form" class="form-select limit-3">
                  <option value="1">نموذج 1</option>
                  <option value="2">نموذج 2</option>
                  <option value="3">نموذج 3</option>
                  <option value="4">نموذج 4</option>
                </select>
              </th>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    {%- endfor %}
  </div>
  <div class="d-flex flex-row-reverse mt-2">
    <button class="btn btn-primary px-8">حفظ</button>
  </div>
</main>
